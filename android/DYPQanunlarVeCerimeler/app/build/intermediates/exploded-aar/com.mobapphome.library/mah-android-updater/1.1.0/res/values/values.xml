    <style name="AppTheme" parent="Theme.AppCompat.Light">
		<!-- Customize your theme here. -->
	</style>
    <style name="MAHBtn1Style" parent="MAHBtnBaseStyle">
		<item name="android:background">@drawable/mah_btn1_background_selector</item>
		<item name="android:textColor">@drawable/mah_btn1_text_selector</item>
	</style>
    <style name="MAHBtn2Style" parent="MAHBtnBaseStyle">
		<item name="android:background">@drawable/mah_btn2_background_selector</item>
		<item name="android:textColor">@drawable/mah_btn2_text_selector</item>
	</style>
